# API Configuration
VITE_API_BASE_URL=https://byte2bite-backend.onrender.com/api/v1

# Environment
VITE_NODE_ENV=production

# Email Service Configuration (for production)
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id

# SMTP Configuration (for backend email service)
VITE_SMTP_HOST=smtp.gmail.com
VITE_SMTP_PORT=587
VITE_SMTP_USER=your_email@gmail.com
VITE_SMTP_PASS=your_app_password

# SendGrid Configuration (alternative)
VITE_SENDGRID_API_KEY=your_sendgrid_api_key
VITE_SENDGRID_FROM_EMAIL=noreply@byte2bite.com

# Google Maps API (if using maps functionality)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id